/**app.wxss**/
@import 'utils/weui.wxss';
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-gouwuche:before { content: "\e633"; }

.icon-chaxun:before { content: "\e608"; }

.icon-weibiaoti--:before { content: "\e60a"; }

.icon-weibiaoti--1:before { content: "\e60c"; }

.icon-weibiaoti--2:before { content: "\e60e"; }

.icon-weibiaoti--3:before { content: "\e610"; }

@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff');
    font-weight: normal;
    font-style: normal;
}
.container{
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 
/*滑动商品*/
.uploadWrap{
  height:270rpx; 
  width:100%;
  display: flex;
  display: -webkit-box;
  flex-direction: column;} 
.upload_Item{
   width: 160rpx; 
   height: 260rpx;
   flex: 1;
   text-align: center;
   display: flex;
   flex-direction: column;
   background:#fff;
   padding:5px 0;
} 
.upload_Item_img{
   width: 160rpx; 
   height: 160rpx;
  }

.upload_Item text{
  display: inline-block;
  margin-bottom:10px; 
  font-size: 13px;
}
.upload_Item .newprice{
  color:red;
  font-weight: 800;
}
.upload_Item .originalprice{
  font-size:10px;
  color: #808080;
  text-decoration:line-through;
}
/*滑动商品结束*/
