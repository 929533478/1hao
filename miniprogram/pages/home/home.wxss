/*旋转图片*/
.navtatle{
  background: #fff;
  padding: 10px;
}
.navcon1{
     position: relative;
     left:5px;
     width: 150px;
     height: 105px;
     transform-style: preserve-3d;
     transform:rotateY(60deg);
         }
.navcon{
     position: relative;
     left:5px;
     width: 150px;
     height: 105px;
     transform-style: preserve-3d;
     transform:rotateY(60deg);
    }
 .navcon .con1,.con4{
  position: absolute;
  
  width: 150px;
  height: 105px;
  background-size: 100%;
        }
.navcon .con2,.con3{
 position: absolute;
 height:105px;
 width: 60px;
 background-size:100%;
}
.navcon .con1{
  transform:translateZ(30px);
}
.navcon .con2{
  transform:translate(120px)rotateY(90deg);
}
.navcon .con3{
   transform:translateX(-29px)rotateY(90deg); 
}
.navcon .con4{
 transform:translateZ(-30px);
}
.navtatle{
 position:relative;
}
.navtatle image{
 position: absolute;
  top: 17px;
  width: 40%;
  height: 95px;
  background-size: 100%;
  right: 10px;
}
  