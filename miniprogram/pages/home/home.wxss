  border-radius: 5px;

}
.waterfall-lists .item-box{
  width: 50%;
  height: 271px;
  text-align: center;
}
.waterfall-lists .item-box image{
  width: 157px;
  height: 157px;
}
.waterfall-lists .item-tag{
  margin-left:10px;
  text-align: center;
  font-size: 13px;
  color: red;
  border: 1px solid red;
  width: 50px;
}
  .waterfall-lists .item-title{
    margin-top: 3px;
    font-size:15px;
    color: #212121;
    font-weight: normal;
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
     width:160px;
     margin-left:4px;
  }
  .waterfall-lists .item-txt-box{
    margin-top: 5px;
    display: flex;
    flex-wrap: wrap;
  }
.waterfall-lists .item-txt-box text{
  width:48%
}
.waterfall-lists .item-txt-box .item-price{
  margin-left: 0px;
  font-size: 16px;
  color: red;
  margin-left: -5px;
}
.waterfall-lists .item-txt-box .item-link{
  font-size:13px;
  color: #999999;
}
