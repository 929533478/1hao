/* miniprogram/pages/home/home.wxss */
.slider{
margin-top: 45px;
}
.slider,.slider image,.slider swiper{
  
  width:100%;
  height:280rpx;  /*响应式的*/
}
.app_top{
  position: fixed;
  top: 0;
  z-index: 9999;
  width:100%;
  background: #7BD2E3;
  display: flex;
  flex-wrap: wrap;
  min-width: 320px;
  max-width: 640px;
  height: 45px;
}
.app_top .topimg{
  width: 20%;
  }
.app_top .topinp input{
 padding-left:50rpx; 
 padding-top: 3px;
}
.app_top .topinp{
  width:60%;
  height:60rpx; 
  background:#E2F6FA;
  border-radius:8rpx;
  font-size:15px;
  margin: 7px 5px 0 0;
}
.app_top .topbtn{
  width: 18%;
}
.topimg image{
  width: 80rpx;
  height: 80rpx;
  margin-left: 3px;
}
.topspan{
  color:white;
  font-size: 10px;
  position: absolute;
  top:10px;
  display:inline-block;
  /*margin: 2px;*/
 
}
.topinp span{
  position: relative;
  bottom:45rpx;
  left: 10rpx;
  font-size: 18px;
  color:#808080;
}
.topbtn span{
  display: inline-block;
  color: white;
  font-size: 50rpx;
  padding-top: 8px;
}
.floor{
  display: flex;
  flex-wrap: wrap;
}
.floor view{
  width:33.333333%;
  height:140px;
}
.floor view image{
  width: 100%;
  height:140px;
}
.header-menu{
  position: relative;
  display: flex;
  flex-wrap: wrap;
  width: 100%;
  background-size: 100%;
  background-image:url('https://6170-appsjop-18c083-1257902140.tcb.qcloud.la/image/carousel/99b7bd3f2f34b51b.jpg?sign=2f2a4e9c8eb43fc1f8ecd5cc33df3774&t=1540635297') ;
  height:80px;
}
.header-menu .header-one{
 width:12%;
 padding:13px 15px 16px 15px;
 
}
.header-menu image{
  margin-top:2px; 
  width: 100%;
